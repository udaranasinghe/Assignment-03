#link the data file
download.file("https://raw.githubusercontent.com/markziemann/SLE712_files/master/week10_files/gene_expression.tsv",destfile = "gene_expression.tsv")
#read in the file making gene accession number the row names
m<-read.table("gene_expression.tsv",header=T,row.names=1)
m
m<-as.data.frame(m)
m$MN<-rowMeans(m)
m
#make the data in decending order
s<-m[order(-m$MN),]
head(s,10)
#count the number of genes with higher average than 10
which(m[,3]<10)
#count the number of rows
dim((m[which(m[,3]<10),]))
pairs(m)
m
